---
layout: main.njk
---
<main>
    <h1 class="title">Blog Tags</h1>
    <ul>
        {%- for tag, posts in collections -%}
            {% if tag !== "all" and tag !== "allPosts" %}
                <li>
                    {% postTagLink availableTags[tag] %}
                </li>
            {% endif %}
        {%- endfor -%}
    </ul>
</main>
